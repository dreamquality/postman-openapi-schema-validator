{
  "name": "postman-openapi-schema-validator",
  "version": "1.1.0",
  "description": "A Node.js utility for validating and integrating OpenAPI schema validations into Postman collections. Supports OpenAPI versions 2.0, 3.0.x. Works as a CLI tool and a library.",
  "main": "src/index.js",
  "bin": {
    "posv": "./cli.js"
  },
  "scripts": {
    "start": "node cli.js",
    "test": "jest",
    "lint": "eslint .",
    "prepare": "npm run lint && npm test",
    "transform": "node transform.js"
  },
  "author": "Alex",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.12.0",
    "js-yaml": "^4.1.0",
    "json-schema-ref-parser": "^9.0.9",
    "json5": "^2.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dreamquality/postman-openapi-schema-validator.git"
  },
  "bugs": {
    "url": "https://github.com/dreamquality/postman-openapi-schema-validator/issues"
  },
  "homepage": "https://github.com/dreamquality/postman-openapi-schema-validator#readme",
  "keywords": [
    "OpenAPI",
    "Postman",
    "schema",
    "validator",
    "API",
    "testing",
    "CLI",
    "validation"
  ],
  "devDependencies": {
    "eslint": "^8.48.0",
    "jest": "^29.7.0"
  }
}
